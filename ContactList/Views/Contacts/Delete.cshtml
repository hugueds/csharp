@model Contact

<h1>Confirm Delete</h1>

<h3>Are you sure you want to delete the Contact @Model.FirstName @Model.LastName ?</h3>

<form asp-action="Destroy" asp-route-id="@Model.ContactId">
    <button type='submit' class="btn btn-danger">Delete</button>
    <a asp-action="Index" class="btn btn-default">Cancel</a>
</form>

