@model Contact

<h1>Edit a Contact</h1>
<!--/Contacts/Create url-->
<form asp-controller="Contacts" asp-action="Edit" class="form-horizontal" role="form">
    <input type="hidden" asp-for="ContactId">        
    @Html.Partial("_EditContact", Model)
    <div class="form-group">
    <div class="col-sm-9 col-sm-offset-3">
        <button type="submit" class="btn btn-primary"> Edit </button>
        <a asp-controller="Contacts" asp-action="Index" class="btn btn-default">Cancel</a>
    </div>
    </div>
</form>